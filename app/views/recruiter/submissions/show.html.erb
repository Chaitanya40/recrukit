<div class="ibox">
	<div class="ibox-title">
		<span><h5>Submission ID: <%= @submission.id %></h5></span>
		<%= link_to "Edit",edit_recruiter_requirement_submission_path(id: @submission.id, requirement_id: @submission.requirement_id ), {class: "btn pull-right"}%>  
	<div>
	<div class="ibox-content">
		<div class="row">
			<div class="col-xs-3">
				<h5>Created at :</h5>
			</div>
			<div class="col-xs-3">
				<span>
					<%= @submission.created_at %>
				</span>
			</div>
		</div>
		<div class="row">
			<div class="col-xs-3">
				<h5>First name</h5>
			</div>
			<div class="col-xs-3">
				<span>
					<%= @submission.profile.first_name %>
				</span>
			</div>
		</div>
		<div class="row">
			<div class="col-xs-3">
				<h5>Last name</h5>
			</div>
			<div class="col-xs-3">
				<span>
					<%= @submission.profile.last_name %>
				</span>
			</div>
		</div>
		<div class="row">
			<div class="col-xs-3">
				<h5>Skills : </h5>
			</div>
			<div class="col-xs-3">
				<%= @profile.skills.join(", ") %>
			</div>
		</div>
		<div class="row">
			<div class="col-xs-3">
				<h5>Location :</h5>
			</div>
			<div class="col-xs-3">
				<%= @profile.city+", "+ @profile.state %>
			</div>
		</div>	
	</div>
	<div class="ibox-content">
		<h4 class="text-center">Resume</h4>
		<iframe src=<%= @profile.resume.url %> width="100%" height="780" style="border: none;"> </iframe>
	</div>
</div>	
		